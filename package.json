{
  "name": "snap-fade-away",
  "version": "1.0.0",
  "main": "dist/index.cjs.js",
  "description": "The effection like Thanos snapping finger",
  "author": "imcuttle <moyuyc95@gmail.com>",
  "scripts": {
    "example": "parcel example.html --open",
    "test": "jest",
    "prepublishOnly": "npm test",
    "test-ci": "npm run clean && npm test",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "dev": "npm run build -- -w",
    "doc": "documentation --github --markdown-toc=false readme index.js -a public -s \"API\" && git add README.md",
    "version": "npm run doc && npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "imcuttle",
    "snap",
    "fade",
    "away",
    "snap-fade-away"
  ],
  "typings": "index.d.ts",
  "license": "MIT",
  "repository": "imcuttle/snap-fade-away",
  "jest": {
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  },
  "module": "index.js",
  "browser": "dist/index.umd.js",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^23.3.14",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "conventional-changelog-cli": "^2.0.21",
    "documentation": "^10.1.0",
    "husky": "^2.2.0",
    "jest": "^23.6.0",
    "parcel": "^1.12.3",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.11.3"
  },
  "dependencies": {
    "html2canvas": "^1.0.0-rc.1",
    "parse-unit": "^1.0.1"
  }
}
